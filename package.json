{
  "name": "epistery-server",
  "version": "1.0.0",
  "description": "ExpressJS server with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/index.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "watch": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^4.17.23",
    "@types/ini": "^4.1.1",
    "cookie-parser": "^1.4.7",
    "ethers": "^6.15.0",
    "express": "^4.18.2",
    "ini": "^5.0.0",
    "kubo-rpc-client": "^5.2.0",
    "node-fetch": "^3.3.2",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
