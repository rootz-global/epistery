{
  "name": "epistery",
  "version": "1.0.0",
  "description": "ExpressJS server with TypeScript",
  "main": "index.mjs",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node test/server.mjs",
    "dev": "ts-node -r tsconfig-paths/register src/api.ts",
    "watch": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "certify": "node --env-file=.env cli.mjs certify"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^4.17.23",
    "@types/ini": "^4.1.1",
    "acme-client": "^5.4.0",
    "aqua-js-sdk": "^3.2.1-44",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.2",
    "ethers": "5.8.0",
    "express": "^4.18.2",
    "ini": "^5.0.0",
    "kubo-rpc-client": "^5.2.0",
    "moment": "^2.30.1",
    "node-fetch": "^3.3.2",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
